  "bin": {
    "dyte-cms-starter": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.tsx",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.33.0",
    "@octokit/rest": "^21.0.0",
    "ejs": "^3.1.10",
