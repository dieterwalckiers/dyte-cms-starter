    "graphql-request": "^7.1.0",
    "ink": "^5.0.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1",
    "tweetnacl-sealedbox-js": "^1.2.0",
    "tweetnacl-util": "^0.15.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/ink-select-input": "^3.0.5",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  },
